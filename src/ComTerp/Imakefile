XCOMM source files for building the ComTerp library
XCOMM

PACKAGE = ComTerp

#ifdef InObjectCodeDir

LIB = ComTerp
CCSUFFIX = c

MakeLibrary($(LIB),$(VERSION))

#define Obj(file) MakeObjectFromSrcFlags(file,-xc++)
#define ObjA(file) MakeObjectFromSrcFlags(file,-xc++ -D__ACE_INLINE__)

Obj(assignfunc)
Obj(boolfunc)
Obj(comfunc)	
ObjA(comterpserv)
Obj(comvalue)
Obj(condfunc)
ObjA(ctrlfunc)
Obj(helpfunc)
Obj(mathfunc)
Obj(numfunc)
Obj(parser)	
Obj(scanner)	
Obj(strmfunc)

#ifdef AceDir
ObjA(comhandler)
#endif

#define Obj31(file) MakeObjectFromSrcFlags(file,-xc++ -Uiv2_6_compatible -I$(TOP)/src/include $(TOP_CCINCLUDES))

Obj31(comterp)
IncludeDependencies()

#ifdef AceDir
InstallLibLink($(ACELIBDIR),$(ACELIBBASE), $(LIBDIR))
#endif

#else

MakeInObjectCodeDir()

#endif
